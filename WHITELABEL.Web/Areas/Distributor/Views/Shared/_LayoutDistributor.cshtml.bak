    <script type="text/javascript">
        $(document).ready(function () {
            CheckDistributorAvailableBalance();
        });  
        function CheckDistributorAvailableBalance() {
            $.ajax({
                url: "/DistributorDashboard/LoadAvailableBalance?area=Distributor",
                cache: false,
                type: "post",
                datatype: "json",
                beforesend: function () {
                },
                success: function (data) {
                    if (data == "0") {
                        $('#txtAvailableBal').text("0");
                    }
                    else {
                        //var AvailableBal = data.CLOSING;
                        var AvailableBal = data.BALANCE;
                        var CreditLimitAmt = data.CREDIT_LIMIT;
                        var ReservedCreditLimitAmt = data.RESERVED_CREDIT_LIMIT;
                        //$("#txtAvailableBal").val(AvailableBal);
                        $('#txtAvailableBal').text(AvailableBal);
                        $('#txtDistCreditLimitAmt').text(CreditLimitAmt);
                        $('#txtDistReservedCreditLimitAmt').text(ReservedCreditLimitAmt);
                    }
                },
                error: function (xhr, status, error) {
                    console.log(status);
                }
            });
        }
    </script>

