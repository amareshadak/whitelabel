</script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#WhitelevelList").change(function () {
            $("#SuperList").empty();
            $("#SuperList").val("--Select--");
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetDistributor")',
                dataType: 'json',
                data: { Disid: $("#WhitelevelList").val() },
                success: function (states) {
                    $("#SuperList").append('<option selected value="">--Select Distributor--</option>');
                    $.each(states, function (i, state) {
                        $("#SuperList").append('<option value="' + state.IDValue + '">' +
                            state.TextValue + '</option>');
                    });
                    var WhiteLevelval = $('#WhitelevelList').val();
                    var Superval = $('#SuperList').val();
                    var dropdownval = $('#MemberList').val();
                    $('.mvc-grid').mvcgrid({
                        //query: 'WhiteLevel=' + WhiteLevelval + '&SuperId=' + Superval + '&Distributor=' + Distributorval + '&Merchant=' + dropdownval + '&Status=' + status,
                        query: 'WhiteLevel=' + WhiteLevelval + '&Distributor=' + Superval + '&Merchant=' + dropdownval,
                        reload: true,
                        reloadStarted: function () {
                            $(".divFooterTotalComm").remove();
                        },
                    });
                },
                error: function (ex) {
                    $("#SuperList").append('<option selected value="">--Select Distributor--</option>');
                    $("#MemberList").append('<option selected value="">--Select Merchant--</option>');
                    var WhiteLevelval = $('#WhitelevelList').val();
                    var Superval = $('#SuperList').val();
                    var dropdownval = $('#MemberList').val();
                    $('.mvc-grid').mvcgrid({
                        query: 'WhiteLevel=' + WhiteLevelval + '&Distributor=' + Superval + '&Merchant=' + dropdownval,
                        //query: 'WhiteLevel=' + WhiteLevelval + '&SuperId=' + Superval + '&Distributor=' + Distributorval + '&Merchant=' + dropdownval + '&Status=' + status,
                        reload: true,
                        reloadStarted: function () {
                            $(".divFooterTotalComm").remove();
                        },
                    });
                }
            });
            return false;
        })
        $("#SuperList").change(function () {
            $("#MemberList").empty();
            $.ajax({
                type: 'POST',
                url: '@Url.Action("GetMerchant")',
                dataType: 'json',
                data: { Disid: $("#SuperList").val() },
                success: function (states) {
                    $("#MemberList").append('<option selected value="">--Select Merchant--</option>');
                    $.each(states, function (i, state) {
                        $("#MemberList").append('<option value="' + state.IDValue + '">' +
                            state.TextValue + '</option>');
                    });
                    var WhiteLevelval = $('#WhitelevelList').val();
                    var Superval = $('#SuperList').val();
                    var dropdownval = $('#MemberList').val();
                    $("#btnExportWhiteLevelrecoard").attr("disabled", false );
                    $('.mvc-grid').mvcgrid({
                        query: 'WhiteLevel=' + WhiteLevelval + '&Distributor=' + Superval + '&Merchant=' + dropdownval,
                        //query: 'WhiteLevel=' + WhiteLevelval + '&SuperId=' + Superval + '&Distributor=' + Distributorval + '&Merchant=' + dropdownval + '&Status=' + status,
                        reload: true,
                        reloadStarted: function () {
                            $(".divFooterTotalComm").remove();
                        },
                    });
                },
                error: function (ex) {
                    $("#MemberList").append('<option selected value="">--Select Merchant--</option>');
                }
            });
            return false;
        })
    });
</script>
<script>
