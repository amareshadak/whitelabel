        var MEm_Id = $("#txtMemberaNamedetails").val();
        $('.mvc-grid').mvcgrid({
            //query: 'MerID=' + MerId + '&DateFrom=' + Date_From + '&Date_To=' + Date_TO,
            query: 'SearchVal=' + MEm_Id,
