<script>
    function myFunction($this) {
        $val = $($this).scrollLeft();
        let selectorValue = $($this).attr("data-scroll-container");
        if (
            $($this).scrollLeft() + $($this).innerWidth() >=
            $($this)[0].scrollWidth
        ) {
            $("[data-scroll-btn='" + selectorValue + "'].btn-next-scroll").attr(
                "disabled",
                true
            );
            // $(".btn-next-scroll").hide();
        } else {
            $("[data-scroll-btn='" + selectorValue + "'].btn-next-scroll").attr(
                "disabled",
                false
            );
        }

        if ($val == 0) {
            $("[data-scroll-btn='" + selectorValue + "'].btn-prev-scroll").attr(
                "disabled",
                true
            );
        } else {
            $("[data-scroll-btn='" + selectorValue + "'].btn-prev-scroll").attr(
                "disabled",
                false
            );
        }
    }

    (function ($) {
        console.log("init-scroll: " + $(".btn-next-scroll").scrollLeft());

        $(document).on("click", ".btn-next-scroll", function () {
            let selectorValue = $(this).attr("data-scroll-btn");
            $("[data-scroll-container='" + selectorValue + "']").animate(
                { scrollLeft: "+=460" },
                500
            );
        });

        $(document).on("click", ".btn-prev-scroll", function () {
            let selectorValue = $(this).attr("data-scroll-btn");
            $("[data-scroll-container='" + selectorValue + "']").animate(
                { scrollLeft: "-=460" },
                500
            );
        });
    })(jQuery);
</script>