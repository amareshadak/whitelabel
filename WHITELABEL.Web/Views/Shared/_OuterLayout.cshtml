    <div id="progress" class="loading hidden">Loading&#8230;</div>

    <script type="text/javascript">
        $(document).ready(function () {
            $('#progress').removeClass("hidden");
            $('#progress').hide();
        })

        function OnSuccess(response) {
            var message = response;
            bootbox.alert({
                message: message,
                size: 'small',
                callback: function () {
                    console.log(message);
                }
            })
            //alert(message);
        }
        function OnFailure(response) {
            alert("Error occured.");
        }

